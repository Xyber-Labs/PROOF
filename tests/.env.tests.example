# E2E Test Configuration
# Copy this file to .env.tests and fill in your values
# All variables use E2E_ prefix

# Service URLs (defaults shown - override if different)
E2E_MARKETPLACE_URL=http://localhost:8001
E2E_SEARCH_ENGINE_URL=http://localhost:8000
E2E_SELLER_URL=http://localhost:8002
E2E_QDRANT_URL=http://localhost:6333

# Wallet configuration (required for payment tests)
# E2E_PRIVATE_KEY=your-private-key-here
# E2E_WALLET_ADDRESS=your-wallet-address-here

# Test configuration
E2E_TIMEOUT_SECONDS=60.0
