# Agent Identity
SELLER_DESCRIPTION=Template Seller Agent

# Marketplace Configuration
MARKETPLACE_BASE_URL=https://marketplace.example.com
MARKETPLACE_TIMEOUT_SECONDS=30

# -----------------------------------------------------------------------------
# LLM Configuration
# -----------------------------------------------------------------------------
# The seller agent uses LangGraph with an LLM for tool selection.
# Required: Set GOOGLE_API_KEY for Google models (GEMINI_2_0_FLASH) or TOGETHER_API_KEY for Together AI models
GOOGLE_API_KEYS='["your-primary-google-api-key", "your-secondary-google-api-key", "your-tertiary-google-api-key"]'
TOGETHER_API_KEYS='["your-primary-together-api-key", "your-secondary-together-api-key"]'


# -----------------------------------------------------------------------------
# MCP Servers Configuration (for connecting to external MCP servers)
# -----------------------------------------------------------------------------
# Configure MCP servers that this seller can connect to as a buyer.
# Example: ArXiv MCP server
MCP_SERVERS__ARXIV__URL="http://localhost:8001/mcp-server/mcp"
MCP_SERVERS__ARXIV__TRANSPORT=streamable_http


# You can add multiple MCP servers:
# MCP_SERVERS__SERVER2__URL="http://localhost:8003/sse"
# MCP_SERVERS__SERVER2__NAME="server2"

# -----------------------------------------------------------------------------
# x402 Protocol Settings (for receiving payments as seller)
# -----------------------------------------------------------------------------
# Enable or disable x402 payment enforcement.
# Set to "off" to disable payments (useful for internal/free deployments).
# Set to "on" to enable payments (default).
SELLER_TEMPLATE_X402_PRICING_MODE=on

# This is the wallet address where you will receive payments.
# IMPORTANT: Replace with your own wallet address.
SELLER_TEMPLATE_X402_PAYEE_WALLET_ADDRESS="0x..."

# (Optional) For authenticated access to the CDP Facilitator on mainnet.
# See: https://docs.cdp.coinbase.com/x402/quickstart-for-sellers#running-on-mainnet
SELLER_TEMPLATE_X402_FACILITATOR_URL="https://x402.org/facilitator/validate"
SELLER_TEMPLATE_X402_CDP_API_KEY_ID=""
SELLER_TEMPLATE_X402_CDP_API_KEY_SECRET=""

# Path to the YAML file containing the pricing configuration for all tools.
# This allows for a much cleaner and more readable pricing setup than using
# nested environment variables.
SELLER_TEMPLATE_X402_PRICING_CONFIG_PATH="tool_pricing.yaml"

# -----------------------------------------------------------------------------
# Buyer x402 Settings (for seller to act as buyer)
# -----------------------------------------------------------------------------
# Private key for the wallet that will be used to pay for services from other MCP servers.
# IMPORTANT: This should be a wallet with funds for making payments.
BUYER_X402_WALLET_PRIVATE_KEY="0x..."

# -----------------------------------------------------------------------------
# Marketplace Registration (auto-register on startup)
# -----------------------------------------------------------------------------
# Enable auto-registration with marketplace
SELLER_REGISTRATION_ENABLED=true

# Marketplace URL for registration
SELLER_REGISTRATION_MARKETPLACE_BASE_URL=http://marketplace:8000

# Agent identity for registration
SELLER_REGISTRATION_AGENT_NAME=Demo Seller Agent
SELLER_REGISTRATION_DESCRIPTION=A demo seller agent providing various services
SELLER_REGISTRATION_TAGS=["demo", "template"]

# URL that marketplace should use to reach this seller (from marketplace's perspective)
SELLER_REGISTRATION_SELLER_BASE_URL=http://seller:8001

# Retry settings for registration
SELLER_REGISTRATION_RETRY_ATTEMPTS=3
SELLER_REGISTRATION_RETRY_DELAY_SECONDS=2.0

# -----------------------------------------------------------------------------
# Seller Template - App Settings
# -----------------------------------------------------------------------------
SELLER_TEMPLATE_HOST=0.0.0.0
SELLER_TEMPLATE_PORT=8001
SELLER_TEMPLATE_LOGGING_LEVEL=INFO
SELLER_TEMPLATE_HOT_RELOAD=false
SELLER_TEMPLATE_EXECUTION_TIMEOUT_SECONDS=300